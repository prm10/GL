name: "lstm_autoencoder"
checkpoint_dir: "/ais/gobi3/u/nitish/mnist/models"
print_after: 100
display_after: 0
validate_after: 1000
save_after: 1000
max_iters: 10000000
dec_seq_length: 10
future_seq_length: 10
binary_data: true
dec_conditional: false
future_conditional: false
lstm {
  name: "lstm_1_enc"
  num_hid: 2048
  has_input: true
  input_dims: 4096
  w_dense {
    init_type: UNIFORM
    scale: 0.01
    epsilon: 0.001
    momentum: 0.9
    l2_decay: 0.0001
    gradient_clip: 0.001
    eps_decay_factor: 0.9
    eps_decay_after: 10000
    rms_prop: true
  }
  w_diag {
    init_type: UNIFORM
    scale: 0.1
    epsilon: 0.001
    momentum: 0.9
    l2_decay: 0.0001
    gradient_clip: 0.001
    eps_decay_factor: 0.9
    eps_decay_after: 10000
    rms_prop: true
  }
  b {
    init_type: LSTM_BIAS
    input_gate_bias: 0
    forget_gate_bias: 5
    input_bias: 0
    output_gate_bias: 0
    epsilon: 0.001
    momentum: 0.9
    gradient_clip: 0.001
    eps_decay_factor: 0.9
    eps_decay_after: 10000
    rms_prop: true
  }
  w_input {
    init_type: UNIFORM
    scale: 0.01
    epsilon: 0.001
    momentum: 0.9
    l2_decay: 0.0001
    gradient_clip: 0.001
    eps_decay_factor: 0.9
    eps_decay_after: 10000
    rms_prop: true
  }
}

